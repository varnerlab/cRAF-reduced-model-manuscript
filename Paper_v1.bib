	Journal = {Cancer Res},
	Number = {14},
	Pages = {3163-3172},
	Title = {{Retinoic Acid Induced Mitogen-activated Protein (MAP)/Extracellular Signal-regulated Kinase (ERK) Kinase-dependent MAP Kinase Activation Needed to Elicit HL-60 Cell Differentiation and Growth Arrest}},
	Url = {http://cancerres.aacrjournals.org/cgi/content/abstract/58/14/3163},
	Volume = {58},
	Year = {1998},
	Bdsk-Url-1 = {http://cancerres.aacrjournals.org/cgi/content/abstract/58/14/3163}}

