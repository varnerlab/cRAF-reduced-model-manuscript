	Pages = {247--258},
	Pmid = {12712404},
	Timestamp = {2007.05.17},
	Title = {Suppression of LNCaP prostate cancer xenograft tumors by a prostate-specific protein tyrosine phosphatase, prostatic acid phosphatase.},
	Url = {http://dx.doi.org/10.1002/pros.10240},
	Volume = {55},
	Year = {2003},
