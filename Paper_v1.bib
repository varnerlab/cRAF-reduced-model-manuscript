	Owner = {Ryan Tasseff},
	Pages = {5477--5485},
	Pmid = {2854198},
	Timestamp = {2007.09.05},
	Title = {The epidermal growth factor receptor from prostate cells is dephosphorylated by a prostate-specific phosphotyrosyl phosphatase.},
	Volume = {8},
	Year = {1988}}

