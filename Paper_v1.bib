	Pii = {S1065-6995(00)90433-5},
	Pmid = {11023645},
	Timestamp = {2008.08.07},
	Title = {Differential responsiveness of prostatic acid phosphatase and prostate-specific antigen mRNA to androgen in prostate cancer cells.},
	Url = {http://dx.doi.org/10.1006/cbir.2000.0433},
