	Pages = {1180-8},
	Pmc = {PMC3310950},
	Pmid = {22182854},
	Pst = {ppublish},
	Title = {{Src inhibitors, PP2 and dasatinib, increase retinoic acid-induced association of Lyn and c-Raf (S259) and enhance MAPK-dependent differentiation of myeloid leukemia cells}},
	Volume = {26},
