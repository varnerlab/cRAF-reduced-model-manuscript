	Keywords = {Inhibitors; Leukemia; Lyn; Resistance; Retinoic acid; c-Raf},
	Mesh = {Antigens, CD11b; Antineoplastic Agents; Antineoplastic Combined Chemotherapy Protocols; Cell Differentiation; Drug Resistance, Neoplasm; G1 Phase Cell Cycle Checkpoints; HL-60 Cells; Humans; Indoles; Leukemia, Promyelocytic, Acute; Molecular Targeted Therapy; Phenols; Phosphatidylinositol 3-Kinase; Protein Kinase Inhibitors; Proto-Oncogene Proteins; Proto-Oncogene Proteins c-raf; Pyrimidines; Signal Transduction; Time Factors; Tretinoin; src-Family Kinases},
