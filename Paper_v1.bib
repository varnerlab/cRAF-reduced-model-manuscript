	Pages = {22096--22104},
	Pmid = {9705354},
	Timestamp = {2007.07.27},
	Title = {Tyrosine phosphorylation of c-ErbB-2 is regulated by the cellular form of prostatic acid phosphatase in human prostate cancer cells.},
	Volume = {273},
	Year = {1998}}

