	Pages = {33--67},
	Pii = {S0083-6729(06)75002-6},
	Pmid = {17368311},
	Timestamp = {2008.02.28},
	Title = {The Intersection Between the Aryl Hydrocarbon Receptor (AhR)- and Retinoic Acid-Signaling Pathways.},
	Url = {http://dx.doi.org/10.1016/S0083-6729(06)75002-6},
