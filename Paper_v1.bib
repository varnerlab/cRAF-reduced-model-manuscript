	Institution = {Department of Biochemistry and Molecular Biology, Bio21 Institute of Molecular Bioscience and Biotechnology, University of Melbourne, 30 Flemington Road, Parkville, VIC 3010, Australia. adhillon@unimelb.edu.au},
	Journal = {Cell Signal},
	Keywords = {14-3-3 Proteins, metabolism; Amino Acid Substitution; Animals; Binding Sites; COS Cells; Cell Line; Cercopithecus aethiops; Extracellular Signal-Regulated MAP Kinases, metabolism; Humans; MAP Kinase Kinase Kinases, metabolism; Mice; Mutation; Phosphorylation; Protein Structure, Tertiary; Proto-Oncogene Proteins c-raf, chemistry/genetics/metabolism; Signal Transduction},
	Language = {eng},
	Medline-Pst = {ppublish},
