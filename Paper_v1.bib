	Doi = {10.1073/pnas.1318496111},
	Journal = {Proc Natl Acad Sci U S A},
	Journal-Full = {Proceedings of the National Academy of Sciences of the United States of America},
	Keywords = {intracellular molecular counting; mammalian gene transcription; nuclear organization; quantitative fluorescence microscopy},
	Mesh = {Cell Line, Tumor; Cell Nucleus; Humans; Microscopy, Fluorescence; Molecular Imaging; Plasmids; RNA Polymerase II; Transcription, Genetic},
	Month = {Jan},
	Number = {2},
	Pages = {681-6},
