	Journal-Full = {The Journal of biological chemistry},
	Mesh = {Antigens, CD14; Base Sequence; CCAAT-Enhancer-Binding Proteins; Cell Differentiation; Cholecalciferol; DNA Primers; DNA-Binding Proteins; Humans; Monocytes; Nuclear Proteins; Promoter Regions, Genetic; Protein Binding; Signal Transduction; Transcriptional Activation; Transforming Growth Factor beta; U937 Cells},
	Month = {Aug},
	Number = {33},
	Pages = {23242-8},
	Pmid = {10438498},
